            <string>/// display health

// set anchors

xanchor = 4;
yanchor = 4;

// clear

with (GUI_healthcrystal) instance_destroy();

// place

var placex = xanchor;

// full crystals
for (var i = 0; i &lt; showhp; i+=6){
    var icon = instance_create(0, 0, GUI_healthcrystal);
    icon.subplacex = placex;
    icon.subplacey = yanchor;
    
    if (showhp - i &lt; 6){
        icon.frame = showhp - i;
    }
    
    placex += 18;
}

if (showhp != obj_player.hp &amp;&amp; global.time mod 2 == 0){
    if (showhp &lt; obj_player.hp) showhp++;
    else showhp--;
}

</string>
